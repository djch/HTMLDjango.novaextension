{
    "identifier": "djch.HTMLDjango",
    "name": "HTML Django",
    "organization": "djch",
    "description": "Syntax highlighting for Django templates (using Tree Sitter grammar)",
    "version": "0.2",
    "license": "MIT",
    "bugs": "https://github.com/djch/HTMLDjango.novaextension/issues",
    "repository": "https://github.com/djch/HTMLDjango.novaextension",
    "keywords": ["django", "python", "dtl", "html", "templates", "syntax"],
    "categories": ["languages"],
    "main": "main.js",
    "commands": {
    "editor": [
      {
        "title": "Django Template Tag Switcher",
        "command": "djangoTagSwitcher",
        "shortcut": "cmd-shift->",
        "when": "editorHasFocus",
        "filters": {
          "syntaxes": ["htmldjango"]
        }
      }
    ]
  }
}